        <!-- Beyond the Desk Section -->
        <div class="mt-8 sm:mt-12">
            <div class="bg-gradient-to-br from-emerald-50 via-teal-50 to-cyan-50 rounded-2xl shadow-2xl p-6 sm:p-8 md:p-12 border-2 border-emerald-200">
