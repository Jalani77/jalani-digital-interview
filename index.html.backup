    <!-- Chat Window -->
    <div id="chatWindow" class="fixed bottom-24 right-6 w-96 h-[600px] bg-white rounded-2xl shadow-2xl z-50 flex flex-col hidden transform transition-all duration-300" style="display: none;">
        <!-- Chat Header -->
        <div class="bg-gradient-to-r from-red-600 to-red-700 text-white p-4 rounded-t-2xl flex items-center justify-between flex-shrink-0">
            <div class="flex items-center">
                <div class="w-10 h-10 bg-white rounded-full flex items-center justify-center mr-3">
                    <svg class="w-6 h-6 text-red-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"></path>
                    </svg>
                </div>
                <div>
                    <h3 class="font-semibold">Recruiter Chat</h3>
                    <p class="text-xs text-red-100">Ask me anything</p>
                </div>
            </div>
            <button id="closeChat" class="text-white hover:text-gray-200 transition-colors">
                <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"></path>
                </svg>
            </button>
        </div>

        <!-- Chat Messages Area - Fixed height, scrollable -->
        <div id="chatMessages" class="flex-1 overflow-y-auto p-4 bg-gray-50 space-y-3" style="min-height: 0;">
            <!-- Initial Welcome Message -->
            <div class="flex justify-start mb-3">
                <div class="bg-gray-200 text-gray-800 rounded-2xl rounded-tl-sm px-4 py-3 max-w-[85%] shadow-sm">
                    <p class="text-sm leading-relaxed">Hi! I'm here to answer your questions. Click one of the buttons below to get started!</p>
                </div>
            </div>
        </div>

        <!-- Input Area - Buttons Only (Text Input Disabled) -->
        <div id="chatInputArea" class="p-4 bg-white border-t border-gray-200 flex-shrink-0">
            <!-- Text input is hidden/disabled - buttons only -->
            <input type="text" id="chatTextInput" class="hidden" disabled>
            
            <!-- Button Container -->
            <div id="chatButtons" class="space-y-2">
                <button class="chat-action-btn w-full text-left px-4 py-3 bg-red-50 hover:bg-red-100 text-red-700 rounded-lg transition-all duration-200 text-sm font-medium shadow-sm hover:shadow-md" data-action="challenge">
                    💥 Tell me about a challenge
                </button>
                <button class="chat-action-btn w-full text-left px-4 py-3 bg-red-50 hover:bg-red-100 text-red-700 rounded-lg transition-all duration-200 text-sm font-medium shadow-sm hover:shadow-md" data-action="perplexity">
                    🤔 Why are you interested in Perplexity?
                </button>
                <button class="chat-action-btn w-full text-left px-4 py-3 bg-red-50 hover:bg-red-100 text-red-700 rounded-lg transition-all duration-200 text-sm font-medium shadow-sm hover:shadow-md" data-action="sql">
                    💻 Show me your SQL skills
                </button>
            </div>
        </div>
    </div>

